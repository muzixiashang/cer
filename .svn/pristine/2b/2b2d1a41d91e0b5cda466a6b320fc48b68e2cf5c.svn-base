package com.liyunet.common.util;

public class LanguageMsg {

	private static String ch="zh";//中文
	private static String en="en";//英文
	
	public static String getLanguageMsg(String languageType,String contype){
		if("ajaxError".equals(contype)){
			return "";
		}else if("ajaxServiceError".equals(contype)){
			return "";
		}else if("ajaxSuccess".equals(contype)){
			return "";
		}else{
			return "";
		}
	}
	
	public static String getLanguageMsg1(String languageType,String contype){
		if(ch.equals(languageType)){
			return "";
		}else if(en.equals(languageType)){
			return "";
		}else{
			return "";
		}
	}
}
